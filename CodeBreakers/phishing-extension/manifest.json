{
  "name": "Phishing Identification Extension",
  "version": "1.1",
  "manifest_version": 3,
  "description": "Checks URLs for phishing before loading.",
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": ["http://127.0.0.1:8080/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
